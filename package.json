    "test": "echo \"Error: no test specified\" && exit 0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/uncodoteam/prettier-config.git"
  },
  "keywords": [
    "prettier",
    "config"
  ],
  "author": "",
  "bugs": {
    "url": "https://github.com/uncodoteam/prettier-config/issues"
  },
  "lint-staged": {
    "*.{json,html,md,css,ts,tsx,js,jsx}": [
      "prettier --write"
    ]
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "homepage": "https://github.com/uncodoteam/prettier-config#readme",
  "devDependencies": {
    "husky": "^9.1.7",
    "lint-staged": "^15.5.1",
    "prettier": "^3.5.3"
  }
